<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="411dp"
        android:layout_height="0dp"
        android:layout_marginTop="1dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/pt_tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="时间：0"
            android:textColor="#FF0000"
            android:textSize="20sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="201dp"
        android:layout_marginEnd="201dp"
        android:layout_marginBottom="25dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/button11"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout13"
        app:layout_constraintStart_toStartOf="@+id/linearLayout13"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout13"
        tools:ignore="MissingConstraints">

    </LinearLayout>

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="409dp"
        android:layout_height="171dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="47dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/mygo" />
    </FrameLayout>

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="wrap_content"
        android:layout_height="89dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <ImageButton
            android:id="@+id/pt_ib_00x00"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:scaleType="center"
            android:src="@drawable/liyu01x01" />

        <ImageButton
            android:id="@+id/pt_ib_00x01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu01x02" />

        <ImageButton
            android:id="@+id/pt_ib_00x02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu01x03" />

        <ImageButton
            android:id="@+id/pt_ib_00x03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu01x04" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10">

        <ImageButton
            android:id="@+id/pt_ib_01x00"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu02x01" />

        <ImageButton
            android:id="@+id/pt_ib_01x01"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu02x02" />

        <ImageButton
            android:id="@+id/pt_ib_01x02"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:scaleType="center"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu02x03" />

        <ImageButton
            android:id="@+id/pt_ib_01x03"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:scaleType="center"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu02x04" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout12"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout11">

        <ImageButton
            android:id="@+id/pt_ib_02x00"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:scaleType="center"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu03x01" />

        <ImageButton
            android:id="@+id/pt_ib_02x01"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:scaleType="center"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu03x02" />

        <ImageButton
            android:id="@+id/pt_ib_02x02"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:scaleType="center"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu03x03" />

        <ImageButton
            android:id="@+id/pt_ib_02x03"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:scaleType="center"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu03x04" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_marginBottom="4dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout12">

        <ImageButton
            android:id="@+id/pt_ib_03x00"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:scaleType="center"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu04x01" />

        <ImageButton
            android:id="@+id/pt_ib_03x01"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:layout_weight="1"
            android:scaleType="center"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu04x02" />

        <ImageButton
            android:id="@+id/pt_ib_03x02"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:scaleType="center"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu04x03" />

        <ImageButton
            android:id="@+id/pt_ib_03x03"
            android:layout_width="105dp"
            android:layout_height="90dp"
            android:scaleType="center"
            android:layout_weight="1"
            android:onClick="hard_onClick"
            android:src="@drawable/liyu04x04" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout13">

    </LinearLayout>

    <Button
        android:id="@+id/button5"
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="21dp"
        android:layout_marginTop="29dp"
        android:onClick="Return"
        android:text="return"
        app:layout_constraintEnd_toStartOf="@+id/button11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout13" />

    <Button
        android:id="@+id/button11"
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="218dp"
        android:onClick="hard_restart"
        android:text="restart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>